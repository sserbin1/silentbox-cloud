    "@fastify/cookie": "^11.0.2",
    "@fastify/websocket": "^10.0.0",
    "@silentbox/shared": "workspace:*",
    "@supabase/supabase-js": "^2.39.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.0",
    "fastify": "^4.25.0",
    "firebase-admin": "^12.0.0",
    "ioredis": "^5.3.0",
    "jsonwebtoken": "^9.0.3",
    "pino": "^8.17.0",
    "pino-pretty": "^10.3.0",
    "socket.io": "^4.7.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.11.0",
